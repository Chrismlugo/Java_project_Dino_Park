<div class="container">
    <table class="dino">
        <tr>
            <th>Name</th>
            <th>Species</th>
            <th>Status</th>
        </tr>
        #foreach($dino in $dinosaurs )
        <tr>
            <td>$dino.getName()</td>
            <td>$dino.getSpecies()</td>
            <td>$dino.hungerLevel()</td>
        </tr>
        #end
    </table>
</div>

